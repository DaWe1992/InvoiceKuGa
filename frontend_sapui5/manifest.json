{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "com.danielwehner.invoicekuga",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{App.title}}",
        "description": "{{App.description}}",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "_version": "1.1.0",
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_belize"
        ]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "rootView": "com.danielwehner.invoicekuga.view.App",
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": {
                "sap.m": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.danielwehner.invoicekuga.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
		"routing": {
			 "config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.danielwehner.invoicekuga.view",
				"controlId": "container",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			 },
			 "routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "customers",
					"name": "customers",
					"target": "customers"
				},
                {
                    "pattern": "invoices",
                    "name": "invoices",
                    "target": "invoices"
                },
                {
                    "pattern": "daily-cash-earnings",
                    "name": "earnings",
                    "target": "earnings"
                },
                {
                    "pattern": "statistics",
                    "name": "statistics",
                    "target": "statistics"
                }
			],
			 "targets": {
				"home": {
				   "viewName": "Home",
				   "viewLevel": 1
				},
				"customers": {
					"viewName": "customer.Customer",
					"viewLevel": 1
				},
                "invoices": {
                    "viewName": "invoice.Invoice",
                    "viewLevel": 1
                },
                "earnings": {
                    "viewName": "earning.Earning",
                    "viewLevel": 1
                },
                "statistics": {
                    "viewName": "statistics.Statistics",
                    "viewLevel": 1
                },
                "notFound": {
				   "viewName": "NotFound",
				   "viewLevel": 2
				}
			}
		}
    }
}
